<p id="notice"><%= notice %></p>

<h1>Rankings</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Ranking import</th>
      <th>Utr</th>
      <th>Reliability</th>
      <th>Utr player</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rankings.each do |ranking| %>
      <tr>
        <td><%= ranking.player_id %></td>
        <td><%= ranking.ranking_import_id %></td>
        <td><%= ranking.utr %></td>
        <td><%= ranking.reliability %></td>
        <td><%= ranking.utr_player_id %></td>
        <td><%= link_to 'Show', ranking %></td>
        <td><%= link_to 'Edit', edit_ranking_path(ranking) %></td>
        <td><%= link_to 'Destroy', ranking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ranking', new_ranking_path %>
