<p id="notice"><%= notice %></p>

<h1>Ranking arkiv</h1>


  <div class="tbl-header">
    <table cellpadding="0" cellspacing="0" border="0">
  <thead>
    <tr>
      <th>MÃ¥ned</th>
      <th>Lastet opp</th>
      <% if current_user %>
        <th colspan="2"></th>
      <% end %>
    </tr>
  </thead>
  </table>
</div>
  <tbody>
    <div class="tbl-content">
    <table cellpadding="0" cellspacing="0" border="0">
      <% n = 0  %>
    <% @ranking_imports.order(rank_date: :desc).each do |ranking_import| %>
      <% if n.odd? %>
      <tr class="odd">
      <% else %>
      <tr>
      <% end %>
      <% n = n + 1 %>
        <td><%= link_to ranking_import.title, ranking_import %></td>
        <td><%= ranking_import.rank_date.strftime("%d-%m-%Y") %></td>
        <% if current_user %>
          <td><%= link_to 'Edit', edit_ranking_import_path(ranking_import) %></td>
          <td><%= link_to 'Destroy', ranking_import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<% if current_user %>
<div class="button">
  <%= link_to "Last opp ny ranking", new_ranking_import_path, :class => "btn" %>
</div>
<% end %>